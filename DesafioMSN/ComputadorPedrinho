public class ComputadorPedrinho{
    public static void main(String[] args){

        MSNMessenger msn = new MSNMessenger();
        msn.enviarMensagem();
        msn.receberMensagem();

        FacebookMessenger face = new FacebookMessenger();
        face.enviarMensagem();
        face.receberMensagem();

        Telegram tel = new Telegram();
        tel.enviarMensagem();
        tel.receberMensagem();
    }
}